function [vt, coord] = coords(path)
%Returns the texture coordinates of an .obj file
    files = openFiles(path);
    file = files(1);

    vt = scanFile(file, 'vt %f %f');
    frewind(file);
    faces = scanFile(file, 'f %f/%f %f/%f %f/%f');
    vertices = faces(1:2:end);
    coord = faces(2:2:end);
    
    vert = zeros(max(vertices), 1);
    for i=1:size(vert)
       vert(i) =  
    end
    
    closeFiles(files);
end

